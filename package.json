{
  "name": "ghub-now",
  "version": "0.2.1",
  "description": "Micro-service for now.sh, that redirects to GitHub (or other) repository page of NPM package",
  "scripts": {
    "test": "prettier-eslint '*.js' --write",
    "posttest": "eslint '*.js' --format codeframe --fix",
    "precommit": "yarn test",
    "start": "micro",
    "dev": "micro-dev"
  },
  "repository": "tunnckoCore/ghub-now",
  "dependencies": {
    "get-pkg": "0.3.0",
    "isobject": "3.0.1",
    "micro": "9.0.2",
    "micro-get": "0.1.1",
    "micro-redirect": "1.0.0",
    "parse-github-url": "1.0.2"
  },
  "devDependencies": {
    "eslint-config-tunnckocore": "0.2.0",
    "husky": "0.14.3",
    "micro-dev": "1.4.0",
    "prettier": "1.16.1",
    "prettier-eslint": "8.2.5",
    "prettier-eslint-cli": "4.4.2"
  }
}
